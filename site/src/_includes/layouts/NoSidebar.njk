<!DOCTYPE html>
<html lang="en" class="initial">
  <head>
    {% include 'head.njk' %}
    <title>{{ title }}</title>
  </head>

  <body>
    {% include 'header.njk' %}
    <main class="layout">
      <div id="grid-main">
        <article id="article" class="stack box">
          <h1>{{ title }}</h1>
           <div class="carbon_ads_mdx_wrapper"></div>
             <script>
              const carbonWrapper = document.querySelector('.carbon_ads_mdx_wrapper');
                const script = document.createElement('script');
                    script.src = '//cdn.carbonads.com/carbon.js?serve=CEBI62JN&placement=cssnanoco';
                        script.async = true;
                            script.id = '_carbonads_js';
                                carbonWrapper.appendChild(script);
             </script>

		      {{ content | safe }}
        </article>
      </div>
    </main>
    {% include 'footer.njk' %}
  </body>
</html>
